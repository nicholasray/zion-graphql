{
  "name": "zion",
  "version": "1.0.0",
  "engines": {
    "node": "7.5.0"
  },
  "description": "",
  "main": "server.js",
  "scripts": {
    "prestart": "npm install --unsafe-perm && npm run migrate",
    "start": "node --harmony-async-await src/server.js",
    "dev": "nodemon -r dotenv/config --harmony-async-await src/server.js",
    "seed": "node -r dotenv/config db/seed.js",
    "migrate": "node -r dotenv/config db/migrate.js",
    "test": "find ./src -name '*Test.js' | xargs mocha -R spec",
    "test:watch": "npm test -- --watch",
    "integration": "find ./test/integration -name '*Test.js' | dotenv_config_path=./.env.test xargs  mocha -r dotenv/config -R spec"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^0.3.1",
    "amqplib": "^0.5.1",
    "axios": "^0.15.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.1",
    "dataloader": "^1.2.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "faker": "^3.1.0",
    "graphql": "^0.9.6",
    "graphql-server-express": "^0.8.5",
    "helmet": "^3.6.0",
    "jwks-rsa": "^1.1.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "pg": "^6.1.0",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dotenv": "^4.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^2.2.0"
  }
}
